    "name": "angular-universal-seed",
    "version": "6.0.0",
    "description": "A seed project for an Angular universal app.",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "travis": "bash ./scripts/travis.sh",
        "build:client-and-server-bundles": "ng build --prod && ng run angular-universal-seed:server:production",
        "build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
        "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
        "serve:ssr": "node dist/server"
